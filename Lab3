#include <stdio.h>
#include<string.h>
int prov(char mass[], int m);
 int main()
{
  int pos,c;
  int i,F,F1,wlen;
  char s[50];
  memset(s,'\0',50);
  F= wlen=F1=0;  
  for(i=0; (c=s[i]=getchar())!=EOF; i++)
  {
  	if(c==' '|| c=='.'||c==','||c=='\t'||c=='\n')
  	{
  		F=0;
  		if (F==1 && prov(s,i) == 0)

			wlen=i;
  		i=(-1);
  		memset(s,'\0',50);
		break;
  	}
	    else 
	    {
	    	if(F==0){
	    	F=1;
	    	pos=i;
	               }
		}
	}
 
  for(i=0; (c=s[i]=getchar())!=EOF; i++)
  {
  	if(c==' '|| c=='.'||c==','||c=='\t'||c=='\n')
  	{
  		
  		if (F==1 && prov(s,i) == 0 && wlen != i)
			F1=1;
		
  		i=(-1);
  		memset(s,'\0',50);
		F=0;
  	}
	    else 
	    {
	    	if(F==0)
	    	F=1;
		}
		if (c=='\n')
		break;
	}
  if (F1==0)
	printf("Words of the same lenght:\n");
  else
	  printf("Words are not the same lenght\n");
  getchar();
	return 0;	
}
int prov(char s[], int m)
{
	int F,i;
	F=0;
	for (i=0; i < m; i++)
	{
		if ((s[i]<'A' || s[i]>'Z') && (s[i]<'a' || s[i]>'z'))
		{
			F=1;
		}
	}
	return F;
}
