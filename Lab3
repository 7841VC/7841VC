#include <iostream>
#include <string>
#include <sstream> 

using namespace std;
 
int main()

{
    string s, word;
    cout << "String:\n";
    getline(cin, s);
    
    stringstream words(s);
    words >> word;
    
    size_t size=word.size(), k=0;
    while (words >> word)
    
    {
        if(word.size()!=size) {k=1; break;}        
    }
    
    if(!k) cout << "YES\n"; 
    else cout << "NO\n";
    
system("pause");

return 0;

}
