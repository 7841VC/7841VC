#include <stdio.h>
#include <iostream>

#define N 10
int main(void)
{
	int x[N]; // массив из N элементов
	
	int i;
	int n = 1;
	int a = 0, b = 0;

	for (i = 0; i < n; i++) {
		if (x[i] % 2) a++;
		else b++;
	}
	if (a < b) {
		for (i = 0; i < N; i++)
			if (x[i] % 2) x[i] = 0;
	}
	else {
		for (i = 0; i < N; i++)
			if (x[i] % 2 == 0) x[i] = 0;
	}
	// вывод массива
	for (i = 0; i < N; i++)
		printf("%d ", x[i]);
	printf("\n");

	getchar();
	return 0;
}
