#include <iostream>
using namespace std;
void delSumb(char* str)
{
    if(*str==0)return;
        char * next = str+1;
        if(*next==0) return;
         while(*str)
         {
            *str=*next;
            str=next;
            ++next;
         }
}
void recursiveDelDigits(char * str)
{   
    if(*str){
        if(*str>='0' && *str <= '9')
        {
            delSumb(str);           
        }
        else ++str;
         recursiveDelDigits(str);
    }
}
int main(int argc, char* argv[])
{
    char str[]="ababa1234galam4aga9687-6dulna056guscka";
        recursiveDelDigits(str);
        cout<<str<<endl;
cout<<endl;
cin.get();
return 0;
}
