#include <iostream>
using namespace std;

int main()
{
	char  str[80];

	char *p;

	int i = 0, j = 0;

	cin.getline(str, 80);

	p = new char[strlen(str)];

	while (str[i] != '\0') {
		if (str[i]<'0' || str[i]>'9')
			p[j++] = str[i];
		i++;
	}

	i = 0;

	while (p[i] != '\0')

		str[i++] = p[i];

	str[i] = '\0';

	cout << str;
	system("pause");
	return 0;
}
